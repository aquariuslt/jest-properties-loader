{
  "name": "jest-properties-loader",
  "version": "1.0.8",
  "description": "jest properties to json loader",
  "main": "lib/index.js",
  "repository": "https://github.com/aquariuslt/jest-properties-loader",
  "author": "Aquariuslt<superaquariuslt@gmail.com>",
  "license": "MIT",
  "keywords": [
    "jest",
    "jest-plugins",
    "jest-loader",
    "properties"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest"
  },
  "dependencies": {
    "properties": "^1.2.1"
  },
  "devDependencies": {
    "@types/jest": "24.0.17",
    "@types/jest": "24.9.1",
    "@types/node": "12.12.28",
    "jest": "24.9.0",
    "lodash": "4.17.15",
    "ts-jest": "24.0.2",
    "ts-node": "8.6.2",
    "tsconfig-paths": "3.8.0",
    "typescript": "3.8.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.ts"
    ],
    "testMatch": [
      "<rootDir>/test/**/*.test.ts"
    ],
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/reports/coverage"
  }
}
